{
  "name": "helloworld",
  "version": "1.0.0",
  "description": "A blank PhoneGap template using Vue.js and Framework7",
  "author": "tommy-carlos williams <tommy@devgeeks.org>",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "build": "node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "unit-watch": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "cordova-build": "npm run build && cordova build",
    "cordova-run": "npm run build && cordova run browser",
    "cordova-run-ios": "npm run build && cordova run ios",
    "cordova-run-android": "npm run build && cordova run android",
    "copy-fonts": "cpy node_modules/framework7-icons/fonts/*.* src/fonts && cpy node_modules/material-design-icons/iconfont/*.{eot,ttf,woff,woff2} src/fonts",
    "postinstall": "npm run copy-fonts"
  },
  "dependencies": {
    "cordova-admob-sdk": "^0.17.0",
    "cordova-android": "^7.0.0",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-admob-free": "^0.17.4",
    "cordova-plugin-background-mode": "^0.7.2",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-geolocation": "^4.0.1",
    "cordova-promise-polyfill": "0.0.2",
    "firebase": "^5.3.0",
    "framework7": "^2.3.1",
    "framework7-icons": "^0.8.9",
    "framework7-vue": "^2.3.0",
    "material-design-icons": "^3.0.1",
    "phonegap-plugin-barcodescanner": "^8.0.0",
    "vue": "^2.5.17",
    "vue2-google-maps": "^0.10.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "chalk": "^2.4.1",
    "chromedriver": "^2.41.0",
    "copy-webpack-plugin": "^4.5.2",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^3.7.1",
    "eslint-config-semistandard": "^8.0.0",
    "eslint-config-standard": "^7.0.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^2.30.1",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.1.0",
    "nightwatch": "^0.9.21",
    "node-notifier": "^5.1.2",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.4.0",
    "phantomjs-prebuilt": "^2.1.3",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-url": "^7.3.2",
    "rimraf": "^2.6.0",
    "selenium-server": "2.53.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.7.2",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^2.11.2",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "cordova": {
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": {
      "cordova-plugin-geolocation": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-admob-free": {},
      "cordova-plugin-camera": {
        "CAMERA_USAGE_DESCRIPTION": "Solo para poder tomar la foto que se enviarÃ¡ como complemento a tu registro de actividad",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": "Almacenar temporalmente la imagen tomada antes de enviarla al servidor"
      },
      "cordova-plugin-file": {},
      "cordova-plugin-background-mode": {},
      "phonegap-plugin-barcodescanner": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      }
    }
  }
}